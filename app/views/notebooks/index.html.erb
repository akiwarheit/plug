<h1>Listing notebooks</h1>

<table>
  <tr>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notebooks.each do |notebook| %>
  <tr>
    <td><%= notebook.description %></td>
    <td><%= button_to 'Show', notebook %></td>
    <td><%= button_to 'Edit', edit_notebook_path(notebook) %></td>
    <td><%= button_to 'Destroy', notebook, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Notebook', new_notebook_path %>
